{
  "name": "xen.one",
  "version": "1.0.0",
  "description": "A simple landing page",
  "repository": "git@github.com:sikhote/xen.one.git",
  "author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@emotion/core": "^10.0.17",
    "babel-plugin-emotion": "^10.0.19",
    "lodash": "^4.17.15",
    "next": "^11.1.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "xo": "^0.25.3"
  },
  "xo": {
    "prettier": true,
    "ignores": [
      "lib/localization"
    ],
    "extends": "xo-react",
    "rules": {
      "unicorn/filename-case": 0,
      "no-console": 2,
      "no-underscore-dangle": 2,
      "import/no-unassigned-import": 0
    },
    "env": [
      "browser",
      "node"
    ],
    "space": 2,
    "settings": {
      "import/core-modules": [
        "styled-jsx",
        "styled-jsx/css"
      ]
    }
  }
}
